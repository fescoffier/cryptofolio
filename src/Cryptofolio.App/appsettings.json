{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Elasticsearch": {
      "IndexFormat": "logs-{0:yyyy.MM}"
    }
  },
  "Paths": [
    "/"
  ],
  "Kafka": {
    "Topics": {
      "HealthChecks": "cryptofolio.healthchecks.app.process"
    },
    "Consumer": {
      "GroupId": "app",
      "AutoOffsetReset": "Earliest",
      "EnablePartitionEof": true,
      "EnableAutoCommit": false
    }
  }
}
